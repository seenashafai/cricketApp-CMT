  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for cricketApp-CMT
	
	pod 'Firebase/Core'
	pod 'Firebase/Database'
	pod 'Firebase/Auth'
	pod 'Firebase/Performance'
	pod 'Fabric', '~> 1.7.2'
	pod 'Crashlytics', '~> 3.9.3'




  target 'cricketApp-CMTTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'cricketApp-CMTUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
